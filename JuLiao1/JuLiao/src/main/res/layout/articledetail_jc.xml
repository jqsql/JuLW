<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mainback"
    android:orientation="vertical"
    android:padding="8dp" >

    <RelativeLayout
        android:id="@+id/shouqi1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#EBEBEB"
        android:padding="10dp" >

        <ImageView
            android:id="@+id/arrow1"
            android:layout_width="15dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/up_arrow" />

        <LinearLayout
            android:id="@+id/matchview1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/hostteamname1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="切尔西"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/vs1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text="VS"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/gustteamname1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="切尔西"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/matchview1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/gamename1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="切尔西"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/matchkey1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="切尔西"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/time1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/free1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:padding="8dp" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/cornerfullgrey"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="2dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/wanfa1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/wanfa_num1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/shengview1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/sheng1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/shenglv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/sheng1"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultsheng1"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/shenglv1"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/pingview1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/ping1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/pinglv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/ping1"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultping1"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/pinglv1"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fuview1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp" >

                <TextView
                    android:id="@+id/fu1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/fulv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/fu1"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultfu1"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/fulv1"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/resultzhong1"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/zhongyi"
                    android:visibility="gone" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/shouqi2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:background="#EBEBEB"
        android:padding="10dp" >

        <ImageView
            android:id="@+id/arrow2"
            android:layout_width="15dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/up_arrow" />

        <LinearLayout
            android:id="@+id/matchview2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/hostteamname2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="切尔西"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/vs2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text="VS"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/gustteamname2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="切尔西"
                android:textColor="@color/textlightgreen"
                android:textSize="14dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/matchview2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/gamename2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="切尔西"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/matchkey2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="切尔西"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/time2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textColor="@color/textlightlightgrey"
                android:textSize="12dp" />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/free2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:padding="8dp" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/cornerfullgrey"
                android:gravity="center"
                android:orientation="vertical"
                android:paddingBottom="2dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/wanfa2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/wanfa_num2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:orientation="horizontal" >

            <RelativeLayout
                android:id="@+id/shengview2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/sheng2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/shenglv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/sheng2"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultsheng2"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/shenglv2"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/pingview2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp"
                android:paddingTop="2dp" >

                <TextView
                    android:id="@+id/ping2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/pinglv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/ping2"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultping2"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/pinglv2"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fuview2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_weight="1"
                android:background="@drawable/cornerfullgrey"
                android:paddingBottom="2dp" >

                <TextView
                    android:id="@+id/fu2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="2dp"
                    android:text="客胜"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/fulv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/fu2"
                    android:layout_centerHorizontal="true"
                    android:text="7.50"
                    android:textColor="@color/textlightgrey"
                    android:textSize="12dp" />

                <ImageView
                    android:id="@+id/resultfu2"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_alignBottom="@id/fulv2"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/zhonggreen"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/resultzhong2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/zhongyi"
                    android:visibility="gone" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="的问"
        android:textColor="@color/textlightgrey"
        android:textSize="14dp" />

</LinearLayout>